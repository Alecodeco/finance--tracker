<div class="row">
  <div class="col-9">
    <table class="table table-bordered table-dark">
      <thead>
        <tr>
          <th scope="col">#</th>
          <th scope="col">Ticker (Symbol)</th>
          <th scope="col">Name</th>
          <th scope="col">Last price</th>
          <th scope="col">Action</th>
        </tr>
      </thead>
      <tbody>
        <% @tracked_stocks.each do |stock| %>
        <tr>
          <th scope="row">1</th>
          <td><%= stock.ticker %></td>
          <td><%= stock.name %></td>
          <td><%= stock.last_price %></td>
          <td>Action here</td>
        </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
